{
  "name": "parse-json",
  "version": "2.2.0",
  "description": "Parse JSON with more helpful errors",
  "license": "MIT",
  "repository": "sindresorhus/parse-json",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && node test.js"
  },
  "files": [
    "index.js",
    "vendor"
  ],
  "keywords": [
    "parse",
    "json",
    "graceful",
    "error",
    "message",
    "humanize",
    "friendly",
    "helpful",
    "string",
    "str"
